    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.1",
    "craco-alias": "^3.0.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
